<fra_cfg>
<!--	xxxxxxxxxxxxxxxxxxxx RMan hardware configuration xxxxxxxxxxxxxxxxxxx -->
	<rman_cfg>
		<fqbits type="Data-streaming" value="2"/>
		<fqbits type="Mailbox" value="2"/>
		<md_create mode="yes"/>
		<bpid type="Data-streaming" value="11"/>
		<bpid type="Doorbell" value="10"/>
		<bpid type="Mailbox" value="11"/>
		<sgbpid value="12"/>
	</rman_cfg>

<!--	xxxxxxxxxxxxxxxxxxxxx FMan port configuration xxxxxxxxxxxxxxxxxxxxxx -->
	<network_cfg>
		<port name="10gec" type="10G" fm="fm0" number="0"/>
		<port name="dtsec0" type="1G" fm="fm0" number="0"/>
		<port name="dtsec1" type="1G" fm="fm0" number="1"/>
		<port name="dtsec2" type="1G" fm="fm0" number="2"/>
		<port name="dtsec3" type="1G" fm="fm0" number="3"/>
		<port name="dtsec4" type="1G" fm="fm0" number="4"/>
	</network_cfg>

<!--	xxxxxxxxxxxxxxxxxxxxxxxx transaction xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->
<!--	transaction field described all the type transaction settings -->
	<transaction name="mbox-general" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="0" mask="3"/>
		<ltr value="0" mask="3" />
		<msglen value="15" mask="1"/>
	</transaction>
	<transaction name="mbox-10gec" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="1" mask="0"/>
		<ltr value="0" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec4" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="0" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec3" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="1" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec2" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="2" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec1" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="3" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec0" type="Mailbox">
		<flowlvl value="0" mask="2"/>
		<mbox value="3" mask="0"/>
		<ltr value="0" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>

<!--	xxxxxxxxxxxxxxxxxxxx RMan to FMan distribution xxxxxxxxxxxxxxxxxxxx -->
	<distribution name="rman_to_fman0_10gec" type="RMAN_TO_FMAN">
		<rio_port number="0" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1500" mode="algorithmic" wq="0"/>
		<transactionref name="mbox-10gec"/>
		<fman_port name="10gec"/>
	</distribution>
	<distribution name="rman_to_fman0_dtsec4" type="RMAN_TO_FMAN">
		<rio_port number="0" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1400" mode="algorithmic" wq="0"/>
		<transactionref name="mbox-dtsec4"/>
		<fman_port name="dtsec4"/>
	</distribution>
	<distribution name="rman_to_fman0_dtsec3" type="RMAN_TO_FMAN">
		<rio_port number="0" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1300" mode="algorithmic" wq="0"/>
		<transactionref name="mbox-dtsec3"/>
		<fman_port name="dtsec3"/>
	</distribution>
	<distribution name="rman_to_fman0_dtsec2" type="RMAN_TO_FMAN">
		<rio_port number="0" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1200" mode="algorithmic" wq="0"/>
		<transactionref name="mbox-dtsec2"/>
		<fman_port name="dtsec2"/>
	</distribution>
	<distribution name="rman_to_fman0_dtsec1" type="RMAN_TO_FMAN">
		<rio_port number="0" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1100" mode="algorithmic" wq="0"/>
		<transactionref name="mbox-dtsec1"/>
		<fman_port name="dtsec1"/>
	</distribution>
	<distribution name="rman_to_fman0_dtsec0" type="RMAN_TO_FMAN">
		<rio_port number="0" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1000" mode="algorithmic" wq="0"/>
		<transactionref name="mbox-dtsec0"/>
		<fman_port name="dtsec0"/>
	</distribution>

<!--	xxxxxxxxxxxxxxxxxxxFMan to RMan distribution xxxxxxxxxxxxxxxxxxxxx -->
	<distribution name="fman_to_rman_10gec" type="FMAN_TO_RMAN">
		<fman_port name="10gec"/>
		<queue wq="0"/>
		<rio_port number="0"/>
		<did value="1"/>
		<transactionref name="mbox-10gec"/>
	</distribution>
	<distribution name="fman_to_rman_dtsec4" type="FMAN_TO_RMAN">
		<fman_port name="dtsec4"/>
		<queue wq="1"/>
		<rio_port number="0"/>
		<did value="1"/>
		<transactionref name="mbox-dtsec4"/>
	</distribution>
	<distribution name="fman_to_rman_dtsec3" type="FMAN_TO_RMAN">
		<fman_port name="dtsec3"/>
		<queue wq="2"/>
		<rio_port number="0"/>
		<did value="1"/>
		<transactionref name="mbox-dtsec3"/>
	</distribution>
	<distribution name="fman_to_rman_dtsec2" type="FMAN_TO_RMAN">
		<fman_port name="dtsec2"/>
		<queue wq="6"/>
		<rio_port number="0"/>
		<did value="1"/>
		<transactionref name="mbox-dtsec2"/>
	</distribution>
	<distribution name="fman_to_rman_dtsec1" type="FMAN_TO_RMAN">
		<fman_port name="dtsec1"/>
		<queue wq="6"/>
		<rio_port number="0"/>
		<did value="1"/>
		<transactionref name="mbox-dtsec1"/>
	</distribution>
	<distribution name="fman_to_rman_dtsec0" type="FMAN_TO_RMAN">
		<fman_port name="dtsec0"/>
		<queue wq="7"/>
		<rio_port number="0"/>
		<did value="1"/>
		<transactionref name="mbox-dtsec0"/>
	</distribution>

<!--	xxxxxxxxxxxxxxxxxxxxxxxxxx Policy xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->
	<policy name="network-srio-network">
		<!-- 10gec packets processing -->
		<dist_order>
			<distributionref name="rman_to_fman0_10gec"/>
		</dist_order>
		<dist_order>
			<distributionref name="fman_to_rman_10gec"/>
		</dist_order>

		<!-- dtsec4 packets processing -->
		<dist_order>
			<distributionref name="rman_to_fman0_dtsec4"/>
		</dist_order>
		<dist_order>
			<distributionref name="fman_to_rman_dtsec4"/>
		</dist_order>

		<!-- dtsec3 packets processing -->
		<dist_order>
			<distributionref name="rman_to_fman0_dtsec3"/>
		</dist_order>
		<dist_order>
			<distributionref name="fman_to_rman_dtsec3"/>
		</dist_order>

		<!-- dtsec2 packets processing -->
		<dist_order>
			<distributionref name="rman_to_fman0_dtsec2"/>
		</dist_order>
		<dist_order>
			<distributionref name="fman_to_rman_dtsec2"/>
		</dist_order>

		<!-- dtsec1 packets processing -->
		<dist_order>
			<distributionref name="rman_to_fman0_dtsec1"/>
		</dist_order>
		<dist_order>
			<distributionref name="fman_to_rman_dtsec1"/>
		</dist_order>

		<!-- dtsec0 packets processing -->
		<dist_order>
			<distributionref name="rman_to_fman0_dtsec0"/>
		</dist_order>
		<dist_order>
			<distributionref name="fman_to_rman_dtsec0"/>
		</dist_order>
	</policy>
</fra_cfg>
