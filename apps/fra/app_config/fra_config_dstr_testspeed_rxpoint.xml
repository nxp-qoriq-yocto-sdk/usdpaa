<fra_cfg>
<!--	xxxxxxxxxxxxxxxxxxxxxRMan hardware configurationxxxxxxxxxxxxxxxxxxxx -->
	<rman_cfg>
		<fqbits type="9" value="2"/>
		<fqbits type="11" value="2"/>
		<md_create mode="yes"/>
		<rxchannel value="0x24"/>
		<bpid type="9" value="11"/>
		<bpid type="10" value="10"/>
		<bpid type="11" value="11"/>
		<sgbpid value="12"/>
	</rman_cfg>
<!--	xxxxxxxxxxxxxxxxxxxxxxxx transaction xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->
<!--	transaction field described all the type transaction settings -->
	<!-- dbell setting -->
	<transaction name="dbell-peer" type="10">
		<flowlvl value="5" mask="1"/>
	</transaction>
	<!-- mailbox settings -->
	<transaction name="mbox-general" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="0" mask="3"/>
		<ltr value="0" mask="3" />
		<msglen value="15" mask="1"/>
	</transaction>
	<transaction name="mbox-10gec" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="1" mask="0"/>
		<ltr value="0" mask="3" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec4" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="0" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec3" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="1" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec2" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="2" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec1" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="2" mask="0"/>
		<ltr value="3" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<transaction name="mbox-dtsec0" type="11">
		<flowlvl value="0" mask="2"/>
		<mbox value="3" mask="0"/>
		<ltr value="0" mask="0" />
		<msglen value="6" mask="1"/>
	</transaction>
	<!-- data streaming settings -->
	<transaction name="dstrm-general" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="0" mask="0xff"/>
		<streamid value="0" mask="0xffff"/>
	</transaction>
	<transaction name="dstr-10gec" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="15" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>
	<transaction name="dstr-dtsec4" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="14" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>
	<transaction name="dstr-dtsec3" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="13" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>
	<transaction name="dstr-dtsec2" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="12" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>
	<transaction name="dstr-dtsec1" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="11" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>
	<transaction name="dstr-dtsec0" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="10" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>
	<transaction name="dstr-peer" type="9">
		<flowlvl value="0" mask="2"/>
		<cos value="20" mask="0"/>
		<streamid value="0" mask="0x1f"/>
	</transaction>

<!--	xxxxxxxxxxxxxxxxxxxxxxxx RX distribution xxxxxxxxxxxxxxxxxxxxxxxxxx -->
<!--	Each rx section corresponds to a ibcu(inbound block classification unit)
	register group -->
	<!-- data streaming rx do processing 1 -->
	<distribution name="dstr_to_fman0_tx5" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1500" mode="algorithmic" wq="0" channel="0x40"/>
		<transactionref name="dstr-10gec"/>
	</distribution>
	<distribution name="dstr_to_fman0_tx4" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1400" mode="algorithmic" wq="0" channel="0x45"/>
		<transactionref name="dstr-dtsec4"/>
	</distribution>
	<distribution name="dstr_to_fman0_tx3" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1300" mode="algorithmic" wq="0" channel="0x44"/>
		<transactionref name="dstr-dtsec3"/>
	</distribution>
	<distribution name="dstr_to_fman0_tx2" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1200" mode="algorithmic" wq="0" channel="0x43"/>
		<transactionref name="dstr-dtsec2"/>
	</distribution>
	<distribution name="dstr_to_fman0_tx1" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1100" mode="algorithmic" wq="0" channel="0x42"/>
		<transactionref name="dstr-dtsec1"/>
	</distribution>
	<distribution name="dstr_to_fman0_tx0" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x1000" mode="algorithmic" wq="0" channel="0x41"/>
		<transactionref name="dstr-dtsec0"/>
	</distribution>
	<!-- data streaming rx do processing 2 -->
	<distribution name="dstr_to_network0" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x2000" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-dtsec0"/>
	</distribution>
	<distribution name="dstr_to_network1" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x2100" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-dtsec1"/>
	</distribution>
	<distribution name="dstr_to_network2" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x2200" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-dtsec2"/>
	</distribution>
	<distribution name="dstr_to_network3" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x2300" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-dtsec3"/>
	</distribution>
	<distribution name="dstr_to_network4" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x2400" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-dtsec4"/>
	</distribution>
	<distribution name="dstr_to_network5" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x2500" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-10gec"/>
	</distribution>
	<!-- mailbox rx do processing 1 -->
	<distribution name="mbox_to_fman0_tx5" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x3500" mode="algorithmic" wq="0" channel="0x40"/>
		<transactionref name="mbox-10gec"/>
	</distribution>
	<distribution name="mbox_to_fman0_tx4" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x3400" mode="algorithmic" wq="0" channel="0x45"/>
		<transactionref name="mbox-dtsec4"/>
	</distribution>
	<distribution name="mbox_to_fman0_tx3" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x3300" mode="algorithmic" wq="0" channel="0x44"/>
		<transactionref name="mbox-dtsec3"/>
	</distribution>
	<distribution name="mbox_to_fman0_tx2" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x3200" mode="algorithmic" wq="0" channel="0x43"/>
		<transactionref name="mbox-dtsec2"/>
	</distribution>
	<distribution name="mbox_to_fman0_tx1" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x3100" mode="algorithmic" wq="0" channel="0x42"/>
		<transactionref name="mbox-dtsec1"/>
	</distribution>
	<distribution name="mbox_to_fman0_tx0" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x3000" mode="algorithmic" wq="0" channel="0x41"/>
		<transactionref name="mbox-dtsec0"/>
	</distribution>
	<!-- test speed -->
	<distribution name="dstr_from_peer" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x4000" mode="algorithmic" wq="0" channel="0x24 0x25 0x26 0x27"/>
		<transactionref name="dstr-peer"/>
	</distribution>
	<distribution name="dbell_from_peer" type="rx">
		<port number="1" mask="1"/>
		<sid value="0" mask="0xff"/>
		<queue base="0x4100" mode="direct" wq="0" channel="0x24"/>
		<transactionref name="dbell-peer"/>
	</distribution>

<!--	xxxxxxxxxxxxxxxxxxxxxxxx TX distribution xxxxxxxxxxxxxxxxxxxxxxxxxx -->
	<!-- data streaming -->
	<distribution name="dstr_to_peer_10gec" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x6500" count="4" wq="0"/>
		<transactionref name="dstr-10gec"/>
	</distribution>
	<distribution name="dstr_to_peer_dtsec4" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x6400" count="4" wq="0"/>
		<transactionref name="dstr-dtsec4"/>
	</distribution>
	<distribution name="dstr_to_peer_dtsec3" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x6300" count="4" wq="0"/>
		<transactionref name="dstr-dtsec3"/>
	</distribution>
	<distribution name="dstr_to_peer_dtsec2" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x6200" count="4" wq="0"/>
		<transactionref name="dstr-dtsec2"/>
	</distribution>
	<distribution name="dstr_to_peer_dtsec1" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x6100" count="4" wq="0"/>
		<transactionref name="dstr-dtsec1"/>
	</distribution>
	<distribution name="dstr_to_peer_dtsec0" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x6000" count="4" wq="0"/>
		<transactionref name="dstr-dtsec0"/>
	</distribution>
	<!-- mailbox -->
	<distribution name="mbox_to_peer_10gec" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x7500" count="4" wq="0"/>
		<transactionref name="mbox-10gec"/>
	</distribution>
	<distribution name="mbox_to_peer_dtsec4" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x7400" count="4" wq="0"/>
		<transactionref name="mbox-dtsec4"/>
	</distribution>
	<distribution name="mbox_to_peer_dtsec3" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x7300" count="4" wq="0"/>
		<transactionref name="mbox-dtsec3"/>
	</distribution>
	<distribution name="mbox_to_peer_dtsec2" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x7200" count="4" wq="0"/>
		<transactionref name="mbox-dtsec2"/>
	</distribution>
	<distribution name="mbox_to_peer_dtsec1" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x7100" count="4" wq="0"/>
		<transactionref name="mbox-dtsec1"/>
	</distribution>
	<distribution name="mbox_to_peer_dtsec0" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x7000" count="4" wq="0"/>
		<transactionref name="mbox-dtsec0"/>
	</distribution>
	<!-- test speed -->
	<distribution name="dstr_to_peer" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x4200" count="4" wq="0"/>
		<transactionref name="dstr-peer"/>
	</distribution>
	<distribution name="dbell_to_peer" type="tx">
		<port number="1"/>
		<did value="1"/>
		<queue base="0x4300" count="1" wq="0"/>
		<transactionref name="dbell-peer"/>
	</distribution>

<!--	xxxxxxxxxxxxxxxxxxxxxxxx FWD distribution xxxxxxxxxxxxxxxxxxxx -->
	<distribution name="network_dtsec0" type="fwd">
		<port type="1G" fm="fm0" number="0" />
	</distribution>
	<distribution name="network_dtsec1" type="fwd">
		<port type="1G" fm="fm0" number="1" />
	</distribution>
	<distribution name="network_dtsec2" type="fwd">
		<port type="1G" fm="fm0" number="2" />
	</distribution>
	<distribution name="network_dtsec3" type="fwd">
		<port type="1G" fm="fm0" number="3" />
	</distribution>
	<distribution name="network_dtsec4" type="fwd">
		<port type="1G" fm="fm0" number="4" />
	</distribution>
	<distribution name="network_10gec" type="fwd">
		<port type="10G" fm="fm0" number="0"/>
	</distribution>

<!--	xxxxxxxxxxxxxxxxxxxxxxxxxx Policy xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->
	<policy name="network-srio-network">
		<!-- 10gec packets processing -->
		<dist_order>
			<distributionref name="network_10gec"/>
			<distributionref name="dstr_to_peer_10gec"/>
		</dist_order>
		<!-- processing 1  10gec-rman<->rman-10gec -->
		<dist_order>
			<distributionref name="dstr_to_fman0_tx5"/>
		</dist_order>
		<!-- processing 2  10gec-rman<->rman-core-10gec -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network5"/>
			<distributionref name="network_10gec"/>
		</dist_order>
		-->
		<!-- 10gec packets processing 3 10gec-rman<->rman-core-drop -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network5"/>
			<droped/>
		</dist_order>
		-->

		<!-- dtsec4 packets processing -->
		<dist_order>
			<distributionref name="network_dtsec4"/>
			<distributionref name="dstr_to_peer_dtsec4"/>
		</dist_order>
		<!-- processing 1  dtsec4-rman<->rman-dtsec4 -->
		<dist_order>
			<distributionref name="dstr_to_fman0_tx4"/>
		</dist_order>
		<!-- processing 2  dtsec4-rman<->rman-core-dtsec4 -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network4"/>
			<distributionref name="network_dtsec4"/>
		</dist_order>
		-->
		<!-- processing 3 dtsec4-rman<->rman-core-drop -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network4"/>
			<droped/>
		</dist_order>
		-->

		<!-- dtsec3 packets processing -->
		<dist_order>
			<distributionref name="network_dtsec3"/>
			<distributionref name="dstr_to_peer_dtsec3"/>
		</dist_order>
		<!-- processing 1  dtsec3-rman<->rman-dtsec3 -->
		<dist_order>
			<distributionref name="dstr_to_fman0_tx3"/>
		</dist_order>
		<!-- processing 2  dtsec3-rman<->rman-core-dtsec3 -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network3"/>
			<distributionref name="network_dtsec3"/>
		</dist_order>
		-->
		<!-- processing 3 dtsec3-rman<->rman-core-drop -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network3"/>
			<droped/>
		</dist_order>
		-->

		<!-- dtsec2 packets processing -->
		<dist_order>
			<distributionref name="network_dtsec2"/>
			<distributionref name="dstr_to_peer_dtsec2"/>
		</dist_order>
		<!-- processing 1  dtsec2-rman<->rman-dtsec2 -->
		<dist_order>
			<distributionref name="dstr_to_fman0_tx2"/>
		</dist_order>
		<!-- processing 2  dtsec2-rman<->rman-core-dtsec2 -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network2"/>
			<distributionref name="network_dtsec2"/>
		</dist_order>
		-->
		<!-- processing 3 dtsec4-rman<->rman-core-drop -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network2"/>
			<droped/>
		</dist_order>
		-->

		<!-- dtsec1 packets processing -->
		<dist_order>
			<distributionref name="network_dtsec1"/>
			<distributionref name="dstr_to_peer_dtsec1"/>
		</dist_order>
		<!-- processing 1  dtsec1-rman<->rman-dtsec1 -->
		<dist_order>
			<distributionref name="dstr_to_fman0_tx1"/>
		</dist_order>
		<!-- processing 2  dtsec1-rman<->rman-core-dtsec1 -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network1"/>
			<distributionref name="network_dtsec1"/>
		</dist_order>
		-->
		<!-- processing 3 dtsec1-rman<->rman-core-drop -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network1"/>
			<droped/>
		</dist_order>
		-->

		<!-- dtsec0 packets processing-->
		<dist_order>
			<distributionref name="network_dtsec0"/>
			<distributionref name="dstr_to_peer_dtsec0"/>
		</dist_order>
		<!-- processing 1  dtsec0-rman<->rman-dtsec0 -->
		<dist_order>
			<distributionref name="dstr_to_fman0_tx0"/>
		</dist_order>
		<!-- processing 2  dtsec0-rman<->rman-core-dtsec0 -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network0"/>
			<distributionref name="network_dtsec0"/>
		</dist_order>
		-->
		<!-- processing 3 dtsec4-rman<->rman-core-drop -->
		<!--
		<dist_order>
			<distributionref name="dstr_to_network0"/>
			<droped/>
		</dist_order>
		-->

		<!-- test speed core-rman<->rman-core-rman -->
		<dist_order>
			<cpu_generate_command/>
			<distributionref name="dbell_to_peer"/>
		</dist_order>
		<dist_order>
			<distributionref name="dbell_from_peer"/>
		</dist_order>
		<!-- send point -->
		<!--
		<dist_order>
			<cpu_generate_packets/>
			<distributionref name="dstr_to_peer"/>
		</dist_order>
		<dist_order>
			<distributionref name="dstr_from_peer"/>
		</dist_order>
		-->
		<!-- receive point -->
		<dist_order>
			<distributionref name="dstr_from_peer"/>
			<distributionref name="dstr_to_peer"/>
		</dist_order>
	</policy>
</fra_cfg>