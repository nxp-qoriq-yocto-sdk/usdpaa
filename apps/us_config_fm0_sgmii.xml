<!--
This fmc configuration file sets the 4 1G ports on the first FMan to use
the policies indicated below.
-->


<cfgdata>
    <config>
	<engine name="fm0">
		<port type="1G" number="0" policy="hash_ipv4_src_dst_policy0"/>
		<port type="1G" number="1" policy="hash_ipv4_src_dst_policy1"/>
		<port type="1G" number="2" policy="hash_ipv4_src_dst_policy2"/>
		<port type="1G" number="3" policy="hash_ipv4_src_dst_policy3"/>
	</engine>
    </config>
</cfgdata>
